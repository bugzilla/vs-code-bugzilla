{
    "name": "bugzilla",
    "displayName": "Bugzilla",
    "description": "configuration and extensions for working on bugzilla",
    "version": "0.0.5",
    "publisher": "dylanwh",
    "repository": "https://github.com/mozilla-bteam/vs-code-bugzilla.git",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Extension Packs"
    ],
    "extensionDependencies": [
        "sfodje.perltidy",
        "sfodje.perlcritic",
        "EditorConfig.EditorConfig",
        "adamvoss.yaml",
        "dbaeumer.vscode-eslint",
        "formulahendry.auto-close-tag",
        "mrmlnc.vscode-apache",
        "samosad.tt"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tt",
                "extensions": [".tmpl"]
            },
            {
                "id": "perl",
                "extensions": [
                    ".cgi"
                ]
            }
        ]
    }
}
